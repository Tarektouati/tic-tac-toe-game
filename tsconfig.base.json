{
	"compilerOptions": {
		"target": "es2022",
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"types": ["vitest/globals"],
		"resolveJsonModule": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
